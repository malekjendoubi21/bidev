<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controles.showprofile"
            styleClass="profile-root"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <HBox spacing="10">
            <ImageView fx:id="profileImage" fitWidth="100" fitHeight="100"/>
            <VBox spacing="5">
                <Label fx:id="firstNameLabel" styleClass="profile-label"/>
                <Label fx:id="lastNameLabel" styleClass="profile-label"/>
            </VBox>
        </HBox>
    </children>
</AnchorPane>
